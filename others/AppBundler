#!/bin/bash
appiconbase64=""
transform_string() {
    local input_string="$1"
    local result=$(echo "$input_string" | tr '[:upper:]' '[:lower:]')
    result=$(echo "$result" | sed 's/ /-/g')
    result=$(echo "$result" | sed 's/[^a-z0-9-]//g')
    echo "$result"
}
appname=$(osascript <<EOF
set test to display dialog "App Name :" default answer "Testing App" buttons {"OK"} default button 1
text returned of test
EOF
)
apppath="$(dirname "$0")/$appname.app/"
mkdir -p "$apppath"
mkdir -p "$apppath/Contents"
mkdir -p "$apppath/Contents/Resources"
mkdir -p "$apppath/Contents/MacOS"
echo "$appiconbase64" | base64 -d > "$apppath/Contents/Resources/AppIcon.icns"
echo -e "#!/bin/bash \nosascript -e 'display dialog \"This is your app\" buttons {\"OK\"} default button 1'" > "$apppath/Contents/MacOS/app"
echo -e "APPL????" > "$apppath/Contents/PkgInfo"
echo -e "<?xml version=\"1.0\" encoding=\"UTF-8\"?>
<!DOCTYPE plist PUBLIC \"-//Apple//DTD PLIST 1.0//EN\" \"http://www.apple.com/DTDs/PropertyList-1.0.dtd\">
<plist version=\"1.0\">
<dict>
	<key>CFBundleExecutable</key>
	<string>app</string>
	<key>CFBundleIconFile</key>
	<string>AppIcon</string>
	<key>CFBundleIconName</key>
	<string>AppIcon</string>
	<key>CFBundleIdentifier</key>
	<string>com.$(transform_string "$appname").app</string>
	<key>CFBundleName</key>
	<string>\"$appname\"</string>
</dict>
</plist>" > "$apppath/Contents/Info.plist"
chmod ugo+x "$apppath/Contents/MacOS/app"